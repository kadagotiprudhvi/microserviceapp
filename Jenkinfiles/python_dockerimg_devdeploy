pipeline {
    agent any
    
    stages {
        stage("ssh & dockerpull") {
            steps {
                sh "ssh ubuntu@18.219.233.229 'sudo docker pull prudhvi575/python_repo:1.1.2'"
            }
        }
        stage("ssh & dockerpull") {
            steps {
                sh "ssh ubuntu@18.219.233.229 'sudo docker run -d prudhvi575/python_repo:1.1.2 '"
            }
        }
    }
}
